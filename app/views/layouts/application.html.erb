<body>
  <div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </a>
        <a class="brand" href="#">Some Store</a>
        <div class="nav-collapse">
          <ul class="nav">
            <li><%= link_to "Store" , stores_path %></li>
            <li><%= link_to "Store Type", store_types_path %></li>
            <li><%= link_to "Store Category", store_categories_path %></li>
            <li><%= link_to "Store Item ", store_items_path  %></li>
            <li><%= link_to "Indent", indents_path %></li>
          </ul>
        </div>
      </div>
    </div>
  </div>

  <div class="container">
<% flash.each do |name, msg| %>
      <div class="alert alert-<%= name == :notice ? "success" : "error" %>">
        <a class="close" data-dismiss="alert">Ã—</a>
        <%= msg %>
      </div>
    <% end %>
    <div class="row">
      <div class="span9"><%= yield %></div>
      <div class="span3">
        <h2>About Us</h2>
